<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context="lesh.egresatecnm.com.fragmentos.Activities.MainActivity">


    <fragment
        android:id="@+id/dataFragment"
        android:name="lesh.egresatecnm.com.fragmentos.Fragments.DataFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="35"
        tools:layout="@layout/fragment_data"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp"></fragment>
    <fragment
        android:id="@+id/detailsFragment"
        android:name="lesh.egresatecnm.com.fragmentos.Fragments.DetailsFragment"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="65"
        tools:layout="@layout/fragment_details"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp"></fragment>

</LinearLayout>
