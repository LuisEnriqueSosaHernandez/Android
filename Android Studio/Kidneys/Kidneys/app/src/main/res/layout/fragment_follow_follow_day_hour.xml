<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FragmentsFollow.FragmentsFollowFollow.FollowFollowDayHourFragment">
    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/card_view_follow_follow_hour"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="4dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        card_view:cardBackgroundColor="@color/colorBlanco"
        card_view:cardCornerRadius="4dp">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="2">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight=".5"
            android:orientation="horizontal"
            android:weightSum="2">
    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:layout_margin="4dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        card_view:cardBackgroundColor="@color/colorBlanco"
        card_view:cardCornerRadius="4dp">

        <TextView
            android:id="@+id/textViewStartFollowFollowHour"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/alice"
            android:gravity="center"
            android:layout_gravity="center"
            android:padding="4dp"
            android:text="@string/horadeinicio"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/colorNegro"
            android:textSize="20sp" />
    </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_margin="4dp"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/colorBlanco"
            card_view:cardCornerRadius="4dp">

            <TextView
                android:id="@+id/textViewEndFollowFollowHour"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/alice"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="4dp"
                android:text="@string/horafinaliza"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="@color/colorNegro"
                android:textSize="20sp" />
        </android.support.v7.widget.CardView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.5"
            android:orientation="vertical"
            android:weightSum="4">
    <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:layout_margin="4dp"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?android:attr/selectableItemBackground"
        card_view:cardBackgroundColor="@color/colorBlanco"
        card_view:cardCornerRadius="4dp">

        <TextView
            android:id="@+id/textViewTypeOfSolutionFollowFollowHour"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/alice"
            android:gravity="center"
            android:padding="4dp"
            android:text="@string/tipodesolucion"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textColor="@color/colorNegro"
            android:textSize="15sp" />
    </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_margin="4dp"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/colorBlanco"
            card_view:cardCornerRadius="4dp">

            <TextView
                android:id="@+id/textViewDrainageFollowFollowHour"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/alice"
                android:gravity="center"
                android:padding="4dp"
                android:text="@string/drenado"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="@color/colorNegro"
                android:textSize="15sp" />
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_margin="4dp"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/colorBlanco"
            card_view:cardCornerRadius="4dp">

            <TextView
                android:id="@+id/textViewUfFollowFollowHour"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/alice"
                android:gravity="center"
                android:padding="4dp"
                android:text="@string/uf"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="@color/colorNegro"
                android:textSize="15sp" />
        </android.support.v7.widget.CardView>
        <android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:card_view="http://schemas.android.com/apk/res-auto"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:layout_margin="4dp"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:attr/selectableItemBackground"
            card_view:cardBackgroundColor="@color/colorBlanco"
            card_view:cardCornerRadius="4dp">

            <TextView
                android:id="@+id/textViewIngestedLiquidFollowFollowHour"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/alice"
                android:gravity="center"
                android:padding="4dp"
                android:text="@string/liquidoingerido"
                android:textAppearance="@style/TextAppearance.AppCompat.Large"
                android:textColor="@color/colorNegro"
                android:textSize="15sp" />
        </android.support.v7.widget.CardView>
        </LinearLayout>
    </LinearLayout>
    </android.support.v7.widget.CardView>
</FrameLayout>